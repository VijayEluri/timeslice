<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">

<module>

    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.user.theme.standard.Standard" />

    <inherits name="com.enokinomi.timeslice.web.top.Top" />

    <entry-point class="com.enokinomi.timeslice.web.top.client.ui.TopLevel" />

    <stylesheet src="ts-default.css" />

    <source path="client" />


	<!-- for testing, reduce GWT compile time, e.g. 202 seconds down to 57 seconds -->
    <!--
    <set-property name="user.agent" value="safari" />
     -->

    <!-- for production, include supported browsers and languages -->
    <!--
     -->
    <extend-property name="locale" values="en"/>
    <extend-property name="locale" values="ja"/>

</module>

